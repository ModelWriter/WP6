      <ownedFlowElements xsi:type="uc:SequenceFlow" Source="//@ownedUseCase.1/@ownedAlternativeFlow.2/@ownedFlowElements.4" Target="//@ownedUseCase.1/@ownedAlternativeFlow.2/@ownedFlowElements.6"/>
      <ownedFlowElements xsi:type="uc:Activity" label="2">
