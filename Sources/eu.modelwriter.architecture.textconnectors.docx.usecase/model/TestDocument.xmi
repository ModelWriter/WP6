      <ownedFlowElements xsi:type="uc:SequenceFlow" Source="//@ownedUseCase.1/@ownedAlternativeFlow.2/@ownedFlowElements.6" Target="//@ownedUseCase.1/@ownedAlternativeFlow.2/@ownedFlowElements.8"/>
      <ownedFlowElements xsi:type="uc:Activity" label="2a">
        <documentation text=" System detects anomalies preventing recovery: " textFormat="tab count:2"/>
