      <ownedFlowElements xsi:type="uc:SequenceFlow" Source="//@ownedUseCase.1/@ownedAlternativeFlow.2/@ownedFlowElements.8" Target="//@ownedUseCase.1/@ownedAlternativeFlow.2/@ownedFlowElements.10"/>
      <ownedFlowElements xsi:type="uc:Activity" label="1">
