      <ownedFlowElements xsi:type="uc:SequenceFlow" Source="//@ownedUseCase.1/@ownedAlternativeFlow.2/@ownedFlowElements.2" Target="//@ownedUseCase.1/@ownedAlternativeFlow.2/@ownedFlowElements.4"/>
