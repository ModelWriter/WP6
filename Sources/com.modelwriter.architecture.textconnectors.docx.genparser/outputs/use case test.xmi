<eu.modelwriter.architecture.textconnectors.docx.parser:Document xmlns:eu.modelwriter.architecture.textconnectors.docx.parser="eu.modelwriter.architecture.textconnectors.docx.parser.documentmodel">
  <ownedParagraph Id="_FxhTwN0rEeS-G88LYN16Ew" name="Use Case UC2: Sign In" rawText="Use Case UC2: Sign In">
    <ownedNode Id="_FxhTwd0rEeS-G88LYN16Ew" name="Primary Actor" rawText=" Student, Lecturer."/>
    <ownedNode Id="_FxrEwN0rEeS-G88LYN16Ew" name="Stakeholders and Interests" rawText="Stakeholders and Interests">
      <ownedNode name="1" rawText="Student: Wants simple user interface, fast response, no system errors."/>
      <ownedNode name="2" rawText="Lecturer: ?"/>
    </ownedNode>
    <ownedNode Id="_Fx01wN0rEeS-G88LYN16Ew" name="Preconditions" rawText="Preconditions">
      <ownedNode name="1" rawText="Student is registered."/>
    </ownedNode>
    <ownedNode Id="_Fx01wd0rEeS-G88LYN16Ew" name="Success Guarantee (Postcondition)" rawText="Success Guarantee (Postcondition)">
      <ownedNode name="1" rawText="Student is logged in."/>
    </ownedNode>
    <ownedNode Id="_Fx01wt0rEeS-G88LYN16Ew" name="Main Success Scenario (or Basic Flow)" rawText="Main Success Scenario (or Basic Flow)">
      <ownedNode name="1" rawText=" Student visits system home page"/>
      <ownedNode name="2" rawText=" System shows home page with login form and sign up button"/>
      <ownedNode name="3" rawText=" Student enters his/her username and password then click login button"/>
      <ownedNode name="4" rawText=" System shows Student&#x2019;s home page"/>
    </ownedNode>
    <ownedNode Id="_Fx01w90rEeS-G88LYN16Ew" name="Extensions (or Alternative Flows)" rawText="Extensions (or Alternative Flows)">
      <ownedNode name="*a" rawText=" At any time, System fails: To support recovery, ensure all transaction sensitive state and events can be recovered from any step of the scenario">
        <ownedNode name="1" rawText=" Student restarts System and requests recovery of prior state"/>
        <ownedNode name="2" rawText=" System reconstructs prior state">
          <ownedNode name="2a" rawText=" System detects anomalies preventing recovery:">
            <ownedNode name="1" rawText=" System signals error to the Student, records the error, and enters a clean state"/>
          </ownedNode>
        </ownedNode>
      </ownedNode>
      <ownedNode Id="_Fx01xN0rEeS-G88LYN16Ew" name="3a" rawText=" Student enters invalid username or password">
        <ownedNode Id="_Fx9_sN0rEeS-G88LYN16Ew" name="1" rawText=" System shows errors and request to Student to retry"/>
        <ownedNode Id="_Fx9_sd0rEeS-G88LYN16Ew" name="2" rawText=" Student enters his/her username and password"/>
      </ownedNode>
      <ownedNode Id="_Fx9_st0rEeS-G88LYN16Ew" name="4a" rawText=" System detects failure to communicate with server">
        <ownedNode Id="_Fx9_s90rEeS-G88LYN16Ew" name="1" rawText=" System signals error and rejects the request"/>
      </ownedNode>
    </ownedNode>
  </ownedParagraph>
